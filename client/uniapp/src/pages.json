{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "hoper",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/homeHL.png",
        "pagePath": "pages/index/index",
        "text": "%tabbar.home%"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/about/about",
        "text": "%tabbar.about%"
      },
      {
        "iconPath": "static/tabbar/IcBaselineCropOriginal.svg",
        "selectedIconPath": "static/tabbar/IcBaselineCropOriginalHL.svg",
        "pagePath": "pages/moment/moment_list",
        "text": "%tabbar.moment%"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/about/i18n",
      "type": "page",
      "style": {
        "navigationBarTitleText": "%app.name%"
      }
    },
    {
      "path": "pages/moment/moment_detail",
      "type": "page",
      "style": {
        "navigationBarTitleText": "瞬间详情"
      }
    },
    {
      "path": "pages/moment/moment_list",
      "type": "page",
      "style": {
        "navigationBarTitleText": "瞬间"
      }
    },
    {
      "path": "pages/weibo/list",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "weibo列表",
        "onReachBottomDistance": 50
      }
    },
    {
      "path": "pages/wopan/list",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "wopan列表",
        "onReachBottomDistance": 50
      }
    },
    {
      "path": "pages/wopan/login",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "wopan登录"
      }
    },
    {
      "path": "pages/wopan/view",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "file-view"
      }
    }
  ],
  "subPackages": []
}
