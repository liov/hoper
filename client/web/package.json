{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "node": "v22.0.0",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "protoc": "node ../../proto/generate.js grpc-web",
    "wasm:build": "wasm-pack build ../../server/rust/wasm --target web",
    "wasm:link": "pnpm link ../../server/rust/wasm/pkg",
    "pc:dev": "vite --config mixin.pc.vite.config.ts --mode dev",
    "pc:build": "run-p type-check pc:build-only",
    "pc:preview": "vite preview --config mixin.pc.vite.config.ts --port 4173",
    "pc:test:unit": "vitest --environment jsdom",
    "pc:test:e2e": "start-server-and-test pc:preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "pc:test:e2e:ci": "start-server-and-test pc:preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "pc:build-only": "vite build --config pc.vite.config.ts --mode prod",
    "h5:dev": "vite --config mixin.h5.vite.config.ts --mode dev",
    "h5:build": "run-p type-check h5:build-only",
    "h5:preview": "vite preview --config mixin.h5.vite.config.ts --port 4173",
    "h5:test:unit": "vitest --environment jsdom",
    "h5:test:e2e": "start-server-and-test h5:preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "h5:test:e2e:ci": "start-server-and-test h5:preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "h5:build-only": "vite build --config h5.vite.config.ts --mode prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "diamon": "pnpm link --global @hopeio/utils"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "@hopeio/utils": "^0.0.5",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "google-protobuf": "^3.21.4",
    "grpc-web": "^1.5.0",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "pinia": "^2.2.2",
    "push.js": "^1.0.12",
    "spark-md5": "^3.0.2",
    "tinymce": "^7.3.0",
    "vant": "^4.9.4",
    "vditor": "^3.10.5",
    "vue": "^3.5.1",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@iconify-json/ant-design": "^1.2.0",
    "@iconify-json/logos": "^1.2.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.5.4",
    "@unocss/preset-attributify": "^0.62.3",
    "@unocss/preset-icons": "^0.62.3",
    "@unocss/preset-uno": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.14.2",
    "eslint": "^9.9.1",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^25.0.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.31.0",
    "typescript": "^5.5.4",
    "unocss": "^0.60.3",
    "unplugin-vue-components": "0.27.0",
    "vite": "^5.2.12",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19",
    "weixin-js-sdk": "^1.6.5"
  }
}
